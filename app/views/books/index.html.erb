<h4>Books</h4>
<div class index-body>
<% @books.each do |book| %>
<tr>
    <td><%= book.title %></td>
    <td><%= book.body %></td>
    <td><%= link_to "Show", book_path(@book.id) %></td>
    <td><%= link_to "Edit", edit_book_path(@book.id) %></td>
    <td><%= link_to "Delete", destroy_book_path(@book.id), method: :delete,"data-confilm" => "Are you sure %></td>
</tr>
</div
<% end %>